game.Lighting.DefaultSky.SkyboxFt = "rbxassetid://6847608302"
game.Lighting.DefaultSky.SkyboxLf = "rbxassetid://6847608608"
game.Lighting.DefaultSky.SkyboxRt = "rbxassetid://6847608986"
